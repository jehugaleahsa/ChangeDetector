<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;NET451</TargetFrameworks>
    <Description>Detect how registered objects change throughout the lifetime of a business process.</Description>
    <Copyright>Copyright © 2015</Copyright>
    <PackageLicenseUrl>https://github.com/jehugaleahsa/ChangeDetector/blob/master/UNLICENSE.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/jehugaleahsa/ChangeDetector</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jehugaleahsa/ChangeDetector.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>change detection versioning comparison compare diff</PackageTags>
    <PackageReleaseNotes>Upgrade to .NET Standard</PackageReleaseNotes>
    <NeutralLanguage>en-US</NeutralLanguage>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>ChangeDetector.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

</Project>
